<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="layoutMatchMatch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>

    </style>

    <style name="layoutWrapWrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="layoutMatchWrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="layoutMatchMatchVertical" parent="layoutMatchMatch">
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>

    </style>

    <style name="layoutMatchMatchHorizontal" parent="layoutMatchMatch">
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center</item>

    </style>

    <style name="layoutWrapWrapVertical" parent="layoutWrapWrap">
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="layoutWrapWrapHorizontal" parent="layoutWrapWrap">
        <item name="android:layout_gravity">center</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="layoutMatchWrapHorizontal" parent="layoutMatchWrap">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="layoutMatchWrapVertical" parent="layoutMatchWrap">
        <item name="android:orientation">vertical</item>
    </style>


    <style name="textViewWrapWrapPoppinsBlack" parent="layoutWrapWrap">
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:fontFamily">@font/open_sans</item>
        <item name="android:textColor">@color/black12</item>
    </style>

    <style name="textViewWrapWrapPoppinsBlackGravityStart" parent="layoutWrapWrap">
        <item name="android:layout_gravity">start</item>
    </style>

    <style name="textViewWrapWrapPoppinsBlack18" parent="textViewWrapWrapPoppinsBlack">
        <item name="fontFamily">@font/open_sans</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:paddingBottom">@dimen/_15sdp</item>
        <item name="android:textSize">@dimen/_14sdp</item>
    </style>

    <style name="textViewWrapWrapPoppinsBlack20" parent="textViewWrapWrapPoppinsBlack18">
        <item name="android:textSize">@dimen/_16sdp</item>
        <item name="android:textFontWeight">600</item>
        <item name="android:textAlignment">center</item>
    </style>


    <style name="spinnerModeDropDown" parent="layoutMatchWrap">
        <item name="android:background">@null</item>
        <item name="android:popupBackground">@drawable/spinner_item_background</item>
        <item name="android:spinnerMode">dropdown</item>
    </style>


    <style name="layoutOfCardViewImage" parent="layoutMatchMatch">
        <item name="android:background">@color/custom_green_light</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>


    <style name="backgroundMain" parent="layoutMatchMatch">

    </style>

    <style name="backgroundPopup">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:background">@drawable/custom_popup_white_rounded_background</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">20dp</item>

    </style>

    <style name="backgroundCard">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:elevation">6dp</item>
        <item name="android:background">@color/card_bk</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">10sp</item>

    </style>


    <style name="layoutOnBoardingMain" parent="layoutMatchMatchVertical">

        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintTop_toTopOf">parent</item>

    </style>

    <style name="onBoardingImage">

        <item name="android:layout_width">250dp</item>
        <item name="android:layout_height">210dp</item>
        <item name="android:layout_marginTop">-6dp</item>
        <item name="android:contentDescription">On Boarding Images</item>

    </style>

    <style name="CustomButtonStyle" parent="@style/Widget.Material3.Button.ElevatedButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/custom_green_dark</item>
        <item name="android:paddingStart">30sp</item>
        <item name="android:paddingEnd">30sp</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>


    <style name="wrapWrapDarkElevatedButtonWithPadding" parent="Widget.Material3.Button.ElevatedButton">

        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/custom_green_primary</item>
        <item name="android:paddingStart">30sp</item>
        <item name="android:paddingEnd">30sp</item>
        <item name = "android:fontFamily"> @font/open_sans</item>

    </style>

    <style name="wrapWrapPrimaryElevatedButtonWithPadding" parent="Widget.Material3.Button.ElevatedButton">

        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/custom_green_primary</item>
        <item name="android:paddingStart">30sp</item>
        <item name="android:paddingEnd">30sp</item>
        <item name = "android:fontFamily"> @font/open_sans</item>

    </style>

    <style name="buttonStyleWrapWrapPrimaryElevatedNoPadding" parent="Widget.Material3.Button.ElevatedButton">
        <item name="android:backgroundTint">@color/custom_green_primary</item>
        <item name="android:paddingStart">0sp</item>
        <item name="android:paddingEnd">0sp</item>

    </style>

    <style name="buttonStyleWrapWrapText" parent="Widget.Material3.Button.TextButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingStart">0sp</item>
        <item name="android:paddingEnd">0sp</item>
        <item name = "android:textSize"> @dimen/_10sdp</item>
        <item name = "android:textColor"> @color/custom_black</item>
        <item name = "android:fontFamily"> @font/poppins_medium</item>
    </style>

    <style name="buttonStylePopupLeft" parent="buttonStyleWrapWrapPrimaryElevatedNoPadding">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name = "android:textColor"> @color/custom_black</item>
        <item name = "android:textSize"> @dimen/_10sdp</item>
        <item name = "android:fontFamily"> @font/poppins_medium</item>
        <item name = "android:layout_weight"> 1 </item>
    </style>

    <style name="materialTextInput">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>

        <item name="android:background">@color/background_color_50</item>
        <item name="android:focusable">true</item>
        <item name="android:fontFamily">@font/poppins_light</item>
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="normalTextInput">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">25dp</item>
        <item name="android:fontFamily">@font/poppins_light</item>

        <item name="android:inputType">phone</item>
        <item name="android:lines">1</item>
        <item name="android:maxLength">11</item>
        <item name="android:minHeight">48dp</item>

        <item name="android:padding">10dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black</item>
    </style>




    <style name="materialTextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="boxStrokeColor">@color/custom_green_primary</item>
        <item name="errorEnabled">true</item>
        <item name="hintTextColor">@color/custom_green_primary</item>
    </style>

    <style name="materialPasswordInput" parent="materialTextInputLayout">

        <item name="android:hint">@string/type_your_password</item>
        <item name="android:inputType">textPassword</item>
        <item name="endIconMode">password_toggle</item>

    </style>

    <style name="materialEmailInput" parent="materialTextInputLayout">
        <item name="android:hint">@string/type_your_email</item>
        <item name="android:inputType">textEmailAddress</item>
    </style>


    <style name="spinnerModeGrades">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">7</item>
        <item name="android:entries">@array/Grades</item>
        <item name="android:gravity">end</item>
    </style>

    <style name="layoutCgpa">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingStart">20dp</item>
        <item name="fontFamily">@font/open_sans</item>
        <item name="android:paddingEnd">20dp</item>

    </style>


    <style name="backgroundDev">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">125dp</item>
        <item name="android:layout_margin">5sp</item>
        <item name="android:background">@drawable/dev_shape</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="imageViewCircular">
        <item name="android:layout_width">@dimen/_80sdp</item>
        <item name="android:layout_height">@dimen/_80sdp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_margin">15sp</item>
        <item name="civ_border_color">#FFFFFF</item>
        <item name="civ_border_width">4dp</item>

    </style>

    <style name="layoutDevDetails">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/_80sdp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_margin">20sp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>

    </style>

    <style name="imageViewDevSocialIcon">
        <item name="android:layout_width">@dimen/_28sdp</item>
        <item name="android:layout_height">@dimen/_28sdp</item>
        <item name="android:layout_marginStart">@dimen/_8sdp</item>
    </style>


    <style name="layoutImageButtonContainer" parent="layoutMatchWrap">

        <item name="android:layout_marginStart">40dp</item>
        <item name="android:layout_marginEnd">40dp</item>
        <item name="android:layout_weight">3</item>
        <item name="android:orientation">vertical</item>

    </style>


    <style name="buttonStyleBasic" parent="textViewWrapWrapPoppinsBlack">

        <item name="android:layout_height">30dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@color/background_color_50</item>
        <item name="android:focusable">true</item>
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
        <item name="android:linksClickable">true</item>
        <item name="android:elevation">10dp</item>
        <item name="android:autoLink">web</item>
    </style>


    <style name="cardViewStyleImageContainer">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">145dp</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:focusable">true</item>
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
        <item name="android:linksClickable">true</item>
        <item name="android:elevation">10dp</item>
        <item name="android:autoLink">web</item>
        <item name="android:clickable">true</item>
        <item name="cardBackgroundColor">@android:color/transparent</item>
        <item name="cardCornerRadius">10dp</item>
        <item name="cardElevation">5dp</item>


    </style>


    <style name="imageViewButton">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">80dp</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:padding">10dp</item>
        <item name="tint">#000</item>
    </style>


    <style name="buttonStylePage" parent="buttonStyleWrapWrapPrimaryElevatedNoPadding">

        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:elevation">10dp</item>
        <item name="android:focusable">true</item>
        <item name="android:fontFamily">@font/open_sans</item>
        <item name="android:textFontWeight">600</item>
        <item name="android:textSize">18sp</item>
        <item name="android:backgroundTint">@color/profile_btn</item>
        <item name="android:paddingStart">14dp</item>
        <item name="android:paddingEnd">10dp</item>
        <item name="android:textAlignment">textStart</item>
        <item name="android:textColor">@color/black12</item>
        <item name="android:visibility">visible</item>
        <item name="iconTint">@color/black12</item>
        <item name="iconSize">24dp</item>
    </style>


    <style name="buttonStylePopup">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/cardview_compat_inset_shadow</item>
        <item name="android:layout_weight">0</item>

        <item name="android:focusable">true</item>
        <item name="android:fontFamily">@font/open_sans</item>
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
        <item name="android:textColor">@color/black</item>
    </style>


    <style name="buttonStyleCard">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/cardview_compat_inset_shadow</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@color/background_color_50</item>
        <item name="android:focusable">true</item>
        <item name="android:fontFamily">@font/poppins_light</item>
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="buttonStyleMaterialOutlined" parent="@style/Widget.MaterialComponents.Button.OutlinedButton.Icon">

        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:fontFamily">@font/poppins_light</item>
        <item name="android:textColor">@color/black</item>
        <item name ="android:layout_margin">2sp</item>
        <item name ="android:layout_weight">1</item>

    </style>


    <style name="buttonStyleFloat" parent="Widget.Design.FloatingActionButton">

        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_margin">20dp</item>
        <item name="android:contentDescription">@string/plus_button</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="srcCompat">@android:drawable/ic_input_add</item>

    </style>

    <style name="Widget.App.Button.OutlinedButton.IconOnly" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="iconPadding">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
        <item name="strokeColor">@color/custom_green_light</item>
    </style>


</resources>